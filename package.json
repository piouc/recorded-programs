  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node app.ts",
    "develop": "npm run js -- -d -w & npm run css -- --verbose -w & npm run static -- --verbose -w & ts-node-dev app.ts -w",
