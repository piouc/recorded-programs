    "static": "babel --copy-files --out-dir dist/public src/static",
    "js": "webpack",
    "css": "postcss --no-map -u postcss-import -u postcss-nested -u postcss-sorting -o dist/index.css src/css/index.css",
    "test": "ava"
  },
